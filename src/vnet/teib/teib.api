option version = "1.0.0";

import "vnet/ip/ip_types.api";
import "vnet/interface_types.api";

/** \brief TEIB Entry
    @param sw_if_index 
*/
typedef teib_entry
{
  vl_api_interface_index_t sw_if_index;
  vl_api_address_t peer;
  vl_api_address_t nh;
  u32 nh_table_id;
};

autoreply define teib_entry_add_del
{
  u32 client_index;
  u32 context;
  u8 is_add;
  vl_api_teib_entry_t entry;
};

define teib_dump
{
  u32 client_index;
  u32 context;
};

define teib_details
{
  u32 context;
  vl_api_teib_entry_t entry;
};

