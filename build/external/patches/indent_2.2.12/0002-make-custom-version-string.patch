From 560e39c6c5a9df382550a089b6f626eab93795b0 Mon Sep 17 00:00:00 2001
From: Andrew Yourtchenko <ayourtch@gmail.com>
Date: Wed, 23 Oct 2019 18:31:05 +0200
Subject: [PATCH] fix the versions of the custom indent

---
 configure | 20 ++++++++++----------
 1 file changed, 10 insertions(+), 10 deletions(-)

diff --git a/configure b/configure
index c527d34..fe92cf8 100755
--- a/configure
+++ b/configure
@@ -580,9 +580,9 @@ MAKEFLAGS=
 # Identity of this package.
 PACKAGE_NAME='GNU Indent'
 PACKAGE_TARNAME='indent'
-PACKAGE_VERSION='2.2.12'
-PACKAGE_STRING='GNU Indent 2.2.12'
-PACKAGE_BUGREPORT='bug-indent@gnu.org'
+PACKAGE_VERSION='2.2.12-vpp'
+PACKAGE_STRING='GNU Indent 2.2.12-vpp'
+PACKAGE_BUGREPORT='vpp-dev@lists.fd.io'
 PACKAGE_URL='http://www.gnu.org/software/indent/'
 
 ac_unique_file="src/indent.c"
@@ -1355,7 +1355,7 @@ if test "$ac_init_help" = "long"; then
   # Omit some internal or obsolete options to make the list less imposing.
   # This message is too long to be a string in the A/UX 3.1 sh.
   cat <<_ACEOF
-\`configure' configures GNU Indent 2.2.12 to adapt to many kinds of systems.
+\`configure' configures GNU Indent 2.2.12-vpp to adapt to many kinds of systems.
 
 Usage: $0 [OPTION]... [VAR=VALUE]...
 
@@ -1426,7 +1426,7 @@ fi
 
 if test -n "$ac_init_help"; then
   case $ac_init_help in
-     short | recursive ) echo "Configuration of GNU Indent 2.2.12:";;
+     short | recursive ) echo "Configuration of GNU Indent 2.2.12-vpp:";;
    esac
   cat <<\_ACEOF
 
@@ -1539,7 +1539,7 @@ fi
 test -n "$ac_init_help" && exit $ac_status
 if $ac_init_version; then
   cat <<\_ACEOF
-GNU Indent configure 2.2.12
+GNU Indent configure 2.2.12-vpp
 generated by GNU Autoconf 2.69
 
 Copyright (C) 2012 Free Software Foundation, Inc.
@@ -2191,7 +2191,7 @@ cat >config.log <<_ACEOF
 This file contains any messages produced by compilers while
 running configure, to aid debugging if configure makes a mistake.
 
-It was created by GNU Indent $as_me 2.2.12, which was
+It was created by GNU Indent $as_me 2.2.12-vpp, which was
 generated by GNU Autoconf 2.69.  Invocation command line was
 
   $ $0 $@
@@ -3060,7 +3060,7 @@ fi
 
 # Define the identity of the package.
  PACKAGE='indent'
- VERSION='2.2.12'
+ VERSION='2.2.12-vpp'
 
 
 cat >>confdefs.h <<_ACEOF
@@ -11151,7 +11151,7 @@ cat >>$CONFIG_STATUS <<\_ACEOF || ac_write_fail=1
 # report actual input values of CONFIG_FILES etc. instead of their
 # values after options handling.
 ac_log="
-This file was extended by GNU Indent $as_me 2.2.12, which was
+This file was extended by GNU Indent $as_me 2.2.12-vpp, which was
 generated by GNU Autoconf 2.69.  Invocation command line was
 
   CONFIG_FILES    = $CONFIG_FILES
@@ -11219,7 +11219,7 @@ _ACEOF
 cat >>$CONFIG_STATUS <<_ACEOF || ac_write_fail=1
 ac_cs_config="`$as_echo "$ac_configure_args" | sed 's/^ //; s/[\\""\`\$]/\\\\&/g'`"
 ac_cs_version="\\
-GNU Indent config.status 2.2.12
+GNU Indent config.status 2.2.12-vpp
 configured by $0, generated by GNU Autoconf 2.69,
   with options \\"\$ac_cs_config\\"
 
-- 
2.17.1

