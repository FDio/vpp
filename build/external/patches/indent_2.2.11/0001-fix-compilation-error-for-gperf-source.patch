 #else
 #ifdef __cplusplus
-inline
 #endif
 #endif
 static unsigned int
@@ -94,7 +92,6 @@ hash_cc (str, len)
 }
 
 #ifdef __GNUC__
-__inline
 #endif
 templ_ty *
 is_reserved_cc (str, len)
