index 02d584e..b17d323 100644
--- a/src/gperf.c
+++ b/src/gperf.c
@@ -38,10 +38,8 @@ error "gperf generated tables don't work with this execution character set. Plea
 /* maximum key range = 39, duplicates = 0 */
 
 #ifdef __GNUC__
-__inline
 #else
 #ifdef __cplusplus
-inline
 #endif
 #endif
 static unsigned int
@@ -93,7 +91,6 @@ hash (str, len)
 }
 
