{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "vpp_details",
    "interfaces"
  ],
  "properties": {
    "vpp_details": {
      "$id": "#/properties/vpp_details",
      "type": "object",
      "title": "Details of VPP deployment",
      "required": [
        "program",
        "version",
        "build_date",
        "build_directory"
      ],
      "properties": {
        "program": {
          "$id": "#/properties/vpp_details/properties/program",
          "type": "string",
          "title": "Program name",
          "default": "",
          "examples": [
            "vpe"
          ],
          "pattern": "^(.*)$"
        },
        "version": {
          "$id": "#/properties/vpp_details/properties/version",
          "type": "string",
          "title": "Full VPP version",
          "default": "",
          "examples": [
            "19.01.1-12~g2f4c7ec~b91"
          ],
          "pattern": "^(.*)$"
        },
        "build_date": {
          "$id": "#/properties/vpp_details/properties/build_date",
          "type": "string",
          "title": "VPP build date",
          "default": "",
          "examples": [
            "Thu Apr 18 12:07:27 EDT 2019"
          ],
          "pattern": "^(.*)$"
        },
        "build_directory": {
          "$id": "#/properties/vpp_details/properties/build_directory",
          "type": "string",
          "title": "VPP build directory",
          "default": "",
          "examples": [
            "/home/jenkins/vpp"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "interfaces": {
      "$id": "#/properties/interfaces",
      "type": "array",
      "title": "Array of interface objects (unsorted)",
      "items": {
        "$id": "#/properties/interfaces/items",
        "type": "object",
        "title": "Returned fields",
        "required": [
          "if_uuid",
          "if_index",
          "if_name",
          "if_type",
          "if_tag",
          "if_mac",
          "if_admin_state",
          "if_link_state",
          "if_link_mtu",
          "if_func",
          "if_sub_dot1ad",
          "if_sub_id",
          "if_tx_bytes",
          "if_tx_packets",
          "if_tx_errors",
          "if_rx_bytes",
          "if_rx_packets",
          "if_rx_errors",
          "if_drops",
          "if_punts"
        ],
        "properties": {
          "if_uuid": {
            "$id": "#/properties/interfaces/items/properties/if_uuid",
            "type": "string",
            "title": "Assigned interface UUID (taken from OpenStack tag)",
            "default": "",
            "examples": [
              "f62bf21c-c23b-4c75-8263-a28907111915"
            ],
            "pattern": "^(.*)$"
          },
          "if_index": {
            "$id": "#/properties/interfaces/items/properties/if_index",
            "type": "integer",
            "title": "Interface index",
            "default": 0,
            "examples": [
              0, 1, 2, 3
            ]
          },
          "if_name": {
            "$id": "#/properties/interfaces/items/properties/if_name",
            "type": "string",
            "title": "Interface name",
            "default": "",
            "examples": [
              "TenGigabitEthernet5e/0/2", "BondEthernet0", "VirtualEthernet0/0/0", "tapcli-3"
            ],
            "pattern": "^(.*)$"
          },
          "if_type": {
            "$id": "#/properties/interfaces/items/properties/if_type",
            "type": "string",
            "title": "Interface type (internal for virtual interfaces, external otherwise)",
            "default": "",
            "examples": [
              "internal", "external"
            ],
            "pattern": "^(.*)$"
          },
          "if_tag": {
            "$id": "#/properties/interfaces/items/properties/if_tag",
            "type": "string",
            "title": "Assigned interface tag",
            "default": "",
            "examples": [
              "net-vpp.physnet:physnet1", "net-vpp.port:509df974-acbb-4c35-ab4b-9ccba56c0a7c"
            ],
            "pattern": "^(.*)$"
          },
          "if_mac": {
            "$id": "#/properties/interfaces/items/properties/if_mac",
            "type": "string",
            "title": "Interface MAC address (if exists)",
            "default": "",
            "examples": [
              "3c:fd:fe:a7:a7:8a"
            ],
            "pattern": "^(.*)$"
          },
          "if_admin_state": {
            "$id": "#/properties/interfaces/items/properties/if_admin_state",
            "type": "integer",
            "title": "Interface admin state (1 - up, 0 - down)",
            "default": 0,
            "examples": [
              1
            ]
          },
          "if_link_state": {
            "$id": "#/properties/interfaces/items/properties/if_link_state",
            "type": "integer",
            "title": "Interface link state (1 - up, 0 - down)",
            "default": 0,
            "examples": [
              1
            ]
          },
          "if_link_mtu": {
            "$id": "#/properties/interfaces/items/properties/if_link_mtu",
            "type": "integer",
            "title": "Interface link MTU",
            "default": 0,
            "examples": [
              9216
            ]
          },
          "if_func": {
            "$id": "#/properties/interfaces/items/properties/if_func",
            "type": "string",
            "title": "Interface function (physical if name contains Gigabit or Bond, or tag contains physnet; instance if name contains Virtual; vservice if name contains tapcli; empty otherwise)",
            "default": "",
            "examples": [
              "", "physical", "instance", "vservice"
            ],
            "pattern": "^(.*)$"
          },
          "if_sub_dot1ad": {
            "$id": "#/properties/interfaces/items/properties/if_sub_dot1ad",
            "type": "integer",
            "title": "Interface sub .1ad",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_sub_id": {
            "$id": "#/properties/interfaces/items/properties/if_sub_id",
            "type": "integer",
            "title": "Interface sub id",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_tx_bytes": {
            "$id": "#/properties/interfaces/items/properties/if_tx_bytes",
            "type": "integer",
            "title": "Interface TX bytes",
            "default": 0,
            "examples": [
              6664
            ]
          },
          "if_tx_packets": {
            "$id": "#/properties/interfaces/items/properties/if_tx_packets",
            "type": "integer",
            "title": "Interface TX packets",
            "default": 0,
            "examples": [
              45
            ]
          },
          "if_tx_errors": {
            "$id": "#/properties/interfaces/items/properties/if_tx_errors",
            "type": "integer",
            "title": "Interface TX errors",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_rx_bytes": {
            "$id": "#/properties/interfaces/items/properties/if_rx_bytes",
            "type": "integer",
            "title": "Interface RX bytes",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_rx_packets": {
            "$id": "#/properties/interfaces/items/properties/if_rx_packets",
            "type": "integer",
            "title": "Interface RX packets",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_rx_errors": {
            "$id": "#/properties/interfaces/items/properties/if_rx_errors",
            "type": "integer",
            "title": "Interface RX errors",
            "default": 0,
            "examples": [
              0
            ]
          },
          "if_drops": {
            "$id": "#/properties/interfaces/items/properties/if_drops",
            "type": "integer",
            "title": "Total interface drops",
            "default": 0,
            "examples": [
              136121746
            ]
          },
          "if_punts": {
            "$id": "#/properties/interfaces/items/properties/if_punts",
            "type": "integer",
            "title": "Total interface punts",
            "default": 0,
            "examples": [
              0
            ]
          }
        }
      }
    }
  }
}