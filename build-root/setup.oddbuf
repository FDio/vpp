set term pag off
loop create
set int state loop0 up
oddbuf enable loop0

packet-generator new {
    name ob
    limit 1
    size 128-128
    interface loop0
    node ethernet-input
    data { IP4: 1.2.3 -> 4.5.6
           UDP: 11.22.33.44 -> 11.22.34.44
           UDP: 1234 -> 2345
           incrementing 114
    }
}
